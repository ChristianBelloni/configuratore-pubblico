{
  "presets": ["@babel/preset-typescript"],
  "plugins": [
    ["babel-plugin-macros"],
    ["@babel/plugin-transform-modules-commonjs"],
    [
      "file-loader",
      {
        "name": "[hash].[ext]",
        "extensions": ["png", "jpg", "jpeg", "gif", "svg"],
        "publicPath": "/dist/imgs",
        "outputPath": "/dist/imgs",
        "context": "",
        "limit": 0
      }
    ]
  ]
}
